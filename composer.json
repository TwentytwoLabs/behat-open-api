{
    "name": "twentytwo-labs/behat-open-api",
    "description": "Behatch contexts",
    "keywords": ["Behat", "Symfony", "OpenApi", "Swagger"],
    "type": "library",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-mbstring": "*",
        "behat/behat": "^3.0",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/property-access": "^2.3|^3.0|^4.0|^5.0|^6.0",
        "symfony/http-foundation": "^2.3|^3.0|^4.0|^5.0|^6.0",
        "friends-of-behat/mink-extension": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "TwentytwoLabs\\BehatOpenApiExtension\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    }
}
